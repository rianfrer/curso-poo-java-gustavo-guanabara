package Aula11;
public class Aula11 {

	public static void main(String[] args) {

		/*Pessoa p1 = new Pessoa(); [ERRO] A classe pessoa não pode ser 
		instanciada pq é uma classe abstrata*/
		
		Visitante v1 = new Visitante(); //Visitante é filha de pessoa mas isso n o torna abstrata tbm
		v1.setNome("Juvenal");
		v1.setIdade(22);
		v1.setSexo("M");
		System.out.println(v1.toString());
		
		Aluno a1 = new Aluno();  // Mesmo caso de visitante
		Bolsista b1 = new Bolsista();
	}

}
