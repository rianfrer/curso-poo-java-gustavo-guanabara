package Aula07;

public class Aula07 {

	public static void main(String[] args) {
		//Ultra Emoji Combat
		Lutador L[] = new Lutador[5];
		L[0] = new Lutador("Pretty Boy", "França", 31, 1.75f, 68.9f, 11, 2, 1);
		L[1] = new Lutador("Putscript", "Brasil", 29, 1.68f, 57.8f, 14, 2, 3);
		L[0].apresentar();
		L[1].apresentar();
		
	}

}
